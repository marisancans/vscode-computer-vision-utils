{
  "name": "computer-vision-utils",
  "displayName": "computer-vision-utils",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.75.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:computer-vision-utils.helloWorld"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.computer-vision-utils",
        "title": "Computer vision utils",
        "enablement": "inDebugMode"
      }
    ],
    "keybindings": [
      {
        "command": "extension.computer-vision-utils",
        "key": "alt+shift+e",
        "when": "inDebugMode"
      }
    ],
    "configuration": {
        "title": "Computer Vision Utils",
        "properties": {
          "cvi.fileManagerExecutable": {
            "type": "string",
            "default": "dolphin",
            "description": "Binary name or path to file manager"
          },
          "cvi.imageViewerExecutable": {
            "type": "string",
            "default": "nomacs",
            "description": "Binary name or path to image viewer"
          }
        }
      }
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^8.0.1",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@types/vscode": "^1.75.0",
    "@typescript-eslint/eslint-plugin": "^5.49.0",
    "@typescript-eslint/parser": "^5.49.0",
    "@vscode/test-electron": "^2.2.2",
    "eslint": "^8.33.0",
    "glob": "^8.1.0",
    "mocha": "^10.1.0",
    "ts-loader": "^9.4.2",
    "typescript": "^4.9.4",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1"
  },
  "dependencies": {
    "is-image": "^3.1.0"
  }
}
